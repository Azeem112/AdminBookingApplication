@model IEnumerable<BookingApplication.Models.CustomerDetail>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BussinessName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BussinessWebsite)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubCatageoryNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BusinessCatageory.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BussinessName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BussinessWebsite)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubCatageoryNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BusinessCatageory.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
